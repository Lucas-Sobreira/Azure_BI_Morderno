{
	"name": "dfExtVendasHotmartDestino",
	"properties": {
		"folder": {
			"name": "1 - Extraction"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsExtVendasHotmartOrigem",
						"type": "DatasetReference"
					},
					"name": "VendasOrigem"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dsExtVendasHotmartDestino",
						"type": "DatasetReference"
					},
					"name": "VendasDestino"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Produto as string,",
				"          Afiliado as string,",
				"          {Nome do Produtor} as string,",
				"          {Documento do Produtor} as string,",
				"          {Meio de Pagamento} as string,",
				"          {Preço do Produto} as string,",
				"          {Preço da Oferta} as string,",
				"          {Tipo de Pagamento} as string,",
				"          {Tipo pagamento oferta} as string,",
				"          Moeda as string,",
				"          {Número da Parcela} as string,",
				"          {Recorrência} as string,",
				"          {Data de Venda} as string,",
				"          {Data de Confirmação} as string,",
				"          Status as string,",
				"          DDD as string,",
				"          Cidade as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> VendasOrigem",
				"VendasOrigem sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Afiliado as string",
				"     ),",
				"     partitionFileNames:['vendasHotmart.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> VendasDestino"
			]
		}
	}
}